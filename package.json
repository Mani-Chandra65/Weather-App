{
  "name": "skyscope-pro",
  "version": "1.0.0",
  "description": "Advanced Weather Intelligence & Analytics Platform",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build",
    "start": "cd server && NODE_ENV=production npm start",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:preview": "cd client && npm run preview",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "clean": "rimraf server/node_modules client/node_modules node_modules",
    "netlify:build": "cd client && npm install && npm run build",
    "render:build": "cd server && npm install",
    "render:start": "cd server && npm start"
  },
  "keywords": [
    "weather",
    "forecast",
    "analytics",
    "react",
    "express",
    "api"
  ],
  "author": "Y Mani Chandra Reddy",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mani-Chandra65/Weather-App"
  }
}
